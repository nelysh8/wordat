  <script src="https://kit.fontawesome.com/ac68d7a452.js" crossorigin="anonymous"></script>

</head>

<body>
  <div class="navbar">
    <div class="navbar_logo">
      <a href="/"> <i class="fa-solid fa-tree text_green fs-2 fw-bold"></i> </a>
      <span class="fs-2 fw-bold text_red"> <%=title%> </span>
    </div>
    <div class="navbar_menu">
    </div>
    <div class="navbar_icon">
    </div>
  </div>

  <div class="container mainbox text-center">
    <div class="container text-center"> <p class="text-start fw-bold text-danger">TOTAL NUMBER : <%= results.length%></p> </div>
    <div class="container text-center">
      <%
      for (var i =0; i<results.length; i++){
        var data = results[i];
        var data1 = JSON.stringify(data);
      %>
        <div class="row border mt-2 exam1">
          <div class="card exam1" style="width: 80rem;">
            <div class="card-body">
              <!--                    <h3 name="wordlist_name"><%=data.Tables_in_testdb%></h3>-->
              <h3 name="wordlist_name"><%=data.Tables_in_oq4p2dxa5zpnk9gu%></h3>
              <p class="card-text">content</p>
              <div class="d-flex justify-content-center">
                <!--                        <a href="/word_manage/<%=data.Tables_in_testdb%>" class="m-2 btn btn-success bg-gradient"> 단어장 관리 </a>-->
                <a href="/word_manage/<%=data.Tables_in_oq4p2dxa5zpnk9gu%>" class="m-2 btn btn-success bg-gradient"> 단어장 관리 </a>
                <!--                        <form action="/list_manage/remove/<%=data.Tables_in_testdb%>" method="post"> <button class="m-2 btn btn-danger bg-gradient"> 단어장 삭제 </button> </form>-->
                <form action="/list_manage/remove/<%=data.Tables_in_oq4p2dxa5zpnk9gu%>" method="post"> <button class="m-2 btn btn-danger bg-gradient"> 단어장 삭제 </button> </form>
              </div>
            </div>
          </div>
        </div>
        <% }; %>
    </div>

    <div class="container text-center">
      <div class="row row-col-2 border-0 mb-5">
        <div class="col-11 border-0"></div>
        <!-- Trigger the modal with a button -->
        <div class="col-1 border-0"><button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#myModal"> + </button></div>
        <!-- Modal -->
        <div id="myModal" class="modal fade" role="dialog">
          <div class="modal-dialog">
            <!-- Modal content-->
            <div class="modal-content">
              <div class="modal-header">
                <h4 class="modal-title">단어장 이름을 입력해주세요</h4>
                <button type="button" class="close" data-dismiss="modal">&times;</button>
              </div>
              <div class="modal-body">
                <form action="/list_manage" method="post">
                  <input type="text" name="newlist_name" class="form-control" placeholder="단어장 이름을 입력해주세요" aria-label="Username" aria-describedby="basic-addon1">
                </form>
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="footbar">
    <div class="footbar_menu border">
      <a href="/">
        <ul>
          <li><i class="fa-solid fa-house fs-1 fw-bold"></i></li>
